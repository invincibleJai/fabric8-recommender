{
  "name": "fabric8-stack-analysis-ui-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "bundle-webpack": "npm run build",
    "clean": "npm run clean:lib && npm run clean:app",
    "clean:lib": "npm run rimraf -- projects/fabric8-stack-analysis-ui/node_modules",
