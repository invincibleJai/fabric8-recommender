    "build": "npm run build:lib && npm run build:app",
    "build:app": "ng build",
    "prebuild:lib": "npm run install:lib",
    "build:lib": "ng build --prod fabric8-stack-analysis-ui",
    "test": "npm run test:lib",
    "pretest": "npm run build:lib",
    "test:app": "ng test --watch=false",
    "test:lib": "ng test fabric8-stack-analysis-ui --watch=false --browsers ChromeHeadlessNoSandbox",
    "test:lib:dev": "ng test fabric8-stack-analysis-ui",
    "rimraf": "rimraf",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "c3": "^0.4.18",
    "core-js": "^2.5.4",
    "ngx-base": "^3.1.1",
    "ngx-bootstrap": "^3.0.1",
    "ngx-fabric8-wit": "^6.20.3",
    "ngx-login-client": "^2.3.1",
    "ngx-modal": "^0.0.29",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.4",
    "@angular-devkit/build-ng-packagr": "~0.7.4",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^3.0.0",
    "protractor": "~5.4.0",
    "patternfly": "^3.28.0",
    "replace": "^1.0.0",
    "rimraf": "2.6.2",
    "semantic-release": "15.9.9",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  },
  "release": {
    "branch": "master",
    "debug": true,
    "pkgRoot": "dist/fabric8-stack-analysis-ui"
  },
